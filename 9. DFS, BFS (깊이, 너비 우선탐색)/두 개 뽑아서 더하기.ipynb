   "source": [
    "from itertools import combinations\n",
    "def solution(numbers):\n",
    "    arr = set()\n",
    "    for comb in combinations(numbers, 2):\n",
    "        arr.add(comb[0] + comb[1])\n",
    "        \n",
    "    return sorted(list(arr))\n",
    "    \n",
    "solution([2,1,3,4,1])"
   ]
  }
